create table "transaction"
(
    id                integer not null
        constraint table_name_pk
            primary key autoincrement,
    carPhoto          blob    not null,
    platePhoto        blob    not null,
    plateNumber       text    not null,
    entranceTimestamp timestamp default CURRENT_TIMESTAMP not null,
    exitTimestamp     timestamp default null,
    employeeName      text    not null
);

